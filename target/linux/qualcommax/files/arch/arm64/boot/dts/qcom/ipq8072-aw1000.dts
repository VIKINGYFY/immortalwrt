/dts-v1/;

#include "ipq8074.dtsi"
#include "ipq8074-ess.dtsi"
#include "ipq8074-nss.dtsi"
#include "ipq8074-common.dtsi"
#include "ipq8074-hk-cpu.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "Arcadyan AW1000";
	compatible = "arcadyan,aw1000", "qcom,ipq8074";

	aliases {
		serial0 = &blsp1_uart5;
		serial1 = &blsp1_uart3;

		led-boot = &led_system_red;
		led-failsafe = &led_system_red;
		led-running = &led_system_blue;
		led-upgrade = &led_system_red;

		ethernet0 = &dp1;
		ethernet1 = &dp2;
		ethernet2 = &dp3;
		ethernet3 = &dp4;
		ethernet4 = &dp5;
		ethernet5 = &dp6;

		label-mac-device = &dp1;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs-append = " root=/dev/ubiblock0_1 earlycon=msm_serial_dm,0x78b3000 consoleblank=0";
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 34 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&tlmm 63 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_system_blue: system-blue {
			gpios = <&tlmm 18 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_BLUE>;
		};

		led_system_red: system-red {
			gpios = <&tlmm 25 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_RED>;
		};
	};

	usb_vbus: regulator-usb-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb_vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpios = <&tlmm 9 GPIO_ACTIVE_LOW>;
		regulator-boot-on;
	};
};

&blsp1_uart5 {
	status = "okay";
	pinctrl-0 = <&serial_4_pins>;
	pinctrl-names = "default";
};

&blsp1_uart1 {
	status = "disabled";
};

/* 启用SPI控制器并配置Flash */
&blsp1_spi1 {
	status = "okay";
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	cs-select = <0>;

	spi_nor@0 {
		compatible = "jedec,spi-nor";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "0:SBL1";
				reg = <0x000000 0x050000>;
				read-only;
			};

			partition@50000 {
				label = "0:MIBIB";
				reg = <0x050000 0x010000>;
				read-only;
			};

			partition@60000 {
				label = "0:BOOTCONFIG";
				reg = <0x060000 0x020000>;
				read-only;
			};

			partition@80000 {
				label = "0:BOOTCONFIG1";
				reg = <0x080000 0x020000>;
				read-only;
			};

			partition@a0000 {
				label = "0:QSEE";
				reg = <0x0a0000 0x180000>;
				read-only;
			};

			partition@220000 {
				label = "0:QSEE_1";
				reg = <0x220000 0x180000>;
				read-only;
			};

			partition@3a0000 {
				label = "0:DEVCFG";
				reg = <0x3a0000 0x010000>;
				read-only;
			};

			partition@3b0000 {
				label = "0:DEVCFG_1";
				reg = <0x3b0000 0x010000>;
				read-only;
			};

			partition@3c0000 {
				label = "0:APDP";
				reg = <0x3c0000 0x010000>;
				read-only;
			};

			partition@3d0000 {
				label = "0:APDP_1";
				reg = <0x3d0000 0x010000>;
				read-only;
			};

			partition@3e0000 {
				label = "0:RPM";
				reg = <0x3e0000 0x040000>;
				read-only;
			};

			partition@420000 {
				label = "0:RPM_1";
				reg = <0x420000 0x040000>;
				read-only;
			};

			partition@460000 {
				label = "0:CDT";
				reg = <0x460000 0x010000>;
				read-only;
			};

			partition@470000 {
				label = "0:CDT_1";
				reg = <0x470000 0x010000>;
				read-only;
			};

			partition@480000 {
				label = "0:APPSBLENV";
				reg = <0x480000 0x010000>;
			};

			partition@490000 {
				label = "0:APPSBL";
				reg = <0x490000 0x0a0000>;
				read-only;
			};

			partition@530000 {
				label = "0:APPSBL_1";
				reg = <0x530000 0x0a0000>;
				read-only;
			};

			partition@5d0000 {
				label = "0:ART";
				reg = <0x5d0000 0x040000>;
				read-only;
			};

			partition@610000 {
				label = "0:ETHPHYFW";
				reg = <0x610000 0x080000>;
				read-only;
			};

		};
	};
};

&qpic_nand {
	status = "okay";

	partitions {
		status = "disabled";
	};

	nand@0 {
		reg = <0>;

		nand-ecc-strength = <8>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			rootfs: partition@0 {
				label = "rootfs";
				reg = <0x00000000 0x03400000>;
			};

			partition@3400000 {
				label = "0:WIFIFW";
				reg = <0x03400000 0x00800000>;
				read-only;
			};

			partition@3c00000 {
				label = "rootfs_1";
				reg = <0x03c00000 0x03400000>;
			};

			partition@7000000 {
				label = "0:WIFIFW_1";
				reg = <0x07000000 0x00800000>;
				read-only;
			};

			partition@7800000 {
				label = "data";
				reg = <0x07800000 0x08800000>;
			};
		};
	};
};

&qusb_phy_1 {
	status = "okay";
	vdd-supply = <&usb_vbus>;
};

&mdio {
	status = "okay";

	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 26 0>;

	ethernet-phy-package@0 {
		compatible = "qcom,qca8075-package";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0>;

		qcom,package-mode = "qsgmii";

		qca8075_0: ethernet-phy@0 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <0>;
		};

		qca8075_1: ethernet-phy@1 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <1>;
		};

		qca8075_2: ethernet-phy@2 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <2>;
		};

		qca8075_3: ethernet-phy@3 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <3>;
		};
	};

	phy4: ethernet-phy@28 {
		compatible = "ethernet-phy-id004d.d101";
		reg = <28>;
	};
	
	/* 添加dp6使用的PHY，地址为7，使用c45协议 */
	phy5: ethernet-phy@7 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <7>;
	};
};

&switch {
	status = "okay";

	switch_lan_bmp = <(ESS_PORT1 | ESS_PORT2 | ESS_PORT3 | ESS_PORT4)>;
	switch_wan_bmp = <(ESS_PORT5 |ESS_PORT6)>;
	switch_mac_mode = <MAC_MODE_QSGMII>;
	switch_mac_mode2 = <MAC_MODE_SGMII_PLUS>;

	qcom,port_phyinfo {
		port@1 {
			port_id = <1>;
			phy_address = <0>;
		};

		port@2 {
			port_id = <2>;
			phy_address = <1>;
		};

		port@3 {
			port_id = <3>;
			phy_address = <2>;
		};

		port@4 {
			port_id = <4>;
			phy_address = <3>;
		};

		port@5 {
			port_id = <5>;
			phy_address = <28>;
			port_mac_sel = "QGMAC_PORT";
		};

		port@6 {
			port_id = <6>;
			phy_address = <7>;
			ethernet-phy-ieee802.3-c45;
		};
	};
};

&edma {
	status = "okay";
};

&dp1 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_0>;
	label = "lan1";
};

&dp2 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_1>;
	label = "lan2";
};

&dp3 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_2>;
	label = "lan3";
};

&dp4 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_3>;
	label = "lan4";
};

&dp5 {
	status = "okay";
	phy-handle = <&phy4>;
	label = "wan";
};

&dp6 {
	status = "okay";
	phy-handle = <&phy5>;
};

&wifi {
	status = "okay";
	qcom,ath11k-fw-memory-mode = <1>;
	qcom,ath11k-calibration-variant = "Arcadyan-AW1000";
};
